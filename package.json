{
  "name": "verdaccio-minio",
  "version": "0.1.0",
  "description": "A verdaccio plugin for storing data in Minio",
  "homepage": "https://github.com/barolab/verdaccio-minio#readme",
  "license": "SEE LICENSE IN LICENSE",
  "keywords": [
    "Verdaccio",
    "S3",
    "Minio"
  ],
  "author": {
    "name": "Romain Bailly",
    "email": "rbailly17000@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/barolab/verdaccio-minio.git"
  },
  "bugs": {
    "url": "https://github.com/barolab/verdaccio-minio/issues"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "postinstall": "bash ./scripts/post-install",
    "example": "bash ./scripts/example",
    "lint": "eslint \"**/*.{js,ts}\"",
    "test": "jest",
    "fmt": "prettier -- --write ./**/*.{js,ts,json}",
    "build": "yarn build:types && yarn build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src/ --out-dir lib --extensions \".ts,.tsx\""
  },
  "dependencies": {
    "@verdaccio/commons-api": "^8.0.0",
    "@verdaccio/streams": "^8.0.0",
    "get-stream": "^5.1.0",
    "minio": "^7.0.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@types/minio": "^7.0.2",
    "@types/node": "^12.7.2",
    "@verdaccio/babel-preset": "^8.0.0",
    "@verdaccio/eslint-config": "^8.0.0",
    "@verdaccio/types": "^8.0.0",
    "eslint": "^6.2.2",
    "prettier": "^1.18.2",
    "typescript": "^3.5.3"
  }
}
